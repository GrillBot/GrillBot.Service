{
    "Logging": {
        "LogLevel": {
            "Default": "Information",
            "Microsoft.AspNetCore": "Information",
            "Microsoft.AspNetCore.Routing": "Warning",
            "Microsoft.AspNetCore.Mvc.Infrastructure": "Warning",
            "Microsoft.EntityFrameworkCore": "Warning",
            "Microsoft.AspNetCore.Mvc.StatusCodeResult": "Warning"
        },
        "Console": {
            "FormatterName": "simple",
            "FormatterOptions": {
                "SingleLine": true,
                "IncludeScopes": true
            }
        }
    },
    "AllowedHosts": "*",
    "ConnectionStrings": {
        "Default": ""
    },
    "MinimalTransactionsForMerge": 5000,
    "ExpirationMonths": 14,
    "SuperReactions": {
        "Min": 5,
        "Max": 20,
        "Cooldown": 15
    },
    "Reactions": {
        "Min": 1,
        "Max": 15,
        "Cooldown": 30
    },
    "Message": {
        "Min": 15,
        "Max": 25,
        "Cooldown": 60,
        "Config": {
            "MinLength": 5
        }
    }
}
